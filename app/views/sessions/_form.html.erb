<%= form_tag login_path do %>
<br />
  <div class="field">
    <%= label_tag :username %>
    <%= text_field_tag :username, params[:username] %>
    <%= label_tag :password %>
    <%= password_field_tag :password, params[:password] %>
  </div>
  <div class="actions">
    <%= submit_tag "Log In" %>
  </div>
<% end %>
